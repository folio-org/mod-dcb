{
  "request": {
    "method": "POST",
    "urlPath": "/circulation/requests",
    "bodyPatterns": [
      {
        "matchesJsonPath": {
          "expression": "$.id",
          "matches": "[0-9a-f-]{36}"
        }
      },
      {
        "matchesJsonPath": "$.requestDate"
      }
    ],
    "headers": {
      "x-okapi-tenant": {
        "equalTo": "test_tenant"
      },
      "x-okapi-token": {
        "equalTo": "dGVzdF9qd3RfdG9rZW4="
      }
    }
  },
  "response": {
    "status": 201,
    "headers": {
      "Content-Type": "application/json"
    },
    "transformers": ["response-template"],
    "body": "{{request.body}}"
  }
}
