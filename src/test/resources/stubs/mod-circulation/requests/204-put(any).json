{
  "request": {
    "method": "PUT",
    "urlPathTemplate": "/circulation/requests/{requestId}",
    "pathParameters": {
      "requestId": {
        "matches": "[0-9a-f-]{36}"
      }
    },
    "bodyPatterns": [
      {
        "matchesJsonPath": {
          "expression": "$.id",
          "matches": "[0-9a-f-]{36}"
        }
      },
      {
        "matchesJsonPath": "$.requestDate"
      }
    ],
    "headers": {
      "x-okapi-tenant": {
        "equalTo": "testtenant"
      },
      "x-okapi-token": {
        "equalTo": "dGVzdF9qd3RfdG9rZW4="
      }
    }
  },
  "response": {
    "status": 204,
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
